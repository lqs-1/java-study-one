<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lqs.mapper.CheckItemMapper">
    <insert id="add" parameterType="com.lqs.pojo.CheckItem">
        insert into t_checkitem(code,name,sex,age,price,type,attention,remark) values (#{code},#{name},#{sex},#{age},#{price},#{type},#{attention},#{remark})
    </insert>
</mapper>